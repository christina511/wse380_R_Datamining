@inproceedings{ ISI:000503475800017,
Author = {Gomez-Lopez, Pedro and Montero, Francisco and Lopez, Maria T.},
Editor = {{Vicente, JMF and AlvarezSanchez, JR and Lopez, FD and Moreo, JT and Adeli, H}},
Title = {{Empowering UX of Elderly People with Parkinson's Disease via BCI Touch}},
Booktitle = {{UNDERSTANDING THE BRAIN FUNCTION AND EMOTIONS, PT I}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2019}},
Volume = {{11486}},
Pages = {{161-170}},
