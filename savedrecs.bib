@inproceedings{ ISI:000258398900069,
Author = {Huber, Wolfram and Vitouch, Peter},
Editor = {{Miesenberger, K and Klaus, J and Zagler, W and Karshmer, A}},
Title = {{Usability and accessibility on the internet: Effects of accessible web
   design on usability}},
