@article{ ISI:000357888800008,
Author = {Moradi, Mohammad and Keyvanpour, MohammadReza},
Title = {{CAPTCHA and its Alternatives: A Review}},
Journal = {{SECURITY AND COMMUNICATION NETWORKS}},
