@article{ ISI:000489128400134,
Author = {Naranjo-Zeledon, Luis and Peral, Jesus and Ferrandez, Antonio and
   Chacon-Rivas, Mario},
Title = {{A Systematic Mapping of Translation-Enabling Technologies for Sign
   Languages}},
