Editor = {{Velasco, C and Weber, G and Barroso, J and Mohamad, Y and Paredes, H}},
Title = {{Low-cost natural interface based on head movements}},
Booktitle = {{PROCEEDINGS OF THE 6TH INTERNATIONAL CONFERENCE ON SOFTWARE DEVELOPMENT
   AND TECHNOLOGIES FOR ENHANCING ACCESSIBILITY AND FIGHTING INFO-EXCLUSION}},
Series = {{Procedia Computer Science}},
